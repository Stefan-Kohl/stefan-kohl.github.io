       summary = "This table serves as a partial substitute for the [BACK] button of your browser.">
  <tr> <td> Back to <a class = "footer" href = "../factint.html">FactInt main page</a>,
                    <a class = "footer" href = "../main.html">main page</a> </td> 
       <td> <p align = "right"> Last modified: {DATE} </p> </td> </tr>
</table>

</body>

</html>
